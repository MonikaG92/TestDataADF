{
	"name": "pipeline1",
	"properties": {
		"activities": [
			{
				"name": "Copy Data1",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Stored Procedure1",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource",
						"sqlReaderStoredProcedureName": "[dbo].[selectdata]",
						"storedProcedureParameters": {
							"colname": {
								"type": "String",
								"value": {
									"value": "@activity('Stored Procedure1').output.colname",
									"type": "Expression"
								}
							},
							"TableName": {
								"type": "String",
								"value": {
									"value": "@pipeline().parameters.SRCNAME",
									"type": "Expression"
								}
							}
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"value": "@activity('Stored Procedure1').output",
						"type": "Expression"
					}
				},
				"inputs": [
					{
						"referenceName": "onpremisedb",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "azuredb",
						"type": "DatasetReference",
						"parameters": {
							"AZUREDBNAME": {
								"value": "@pipeline().parameters.SRCNAME",
								"type": "Expression"
							}
						}
					}
				]
			},
			{
				"name": "Stored Procedure1",
				"type": "SqlServerStoredProcedure",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"storedProcedureName": "[dbo].[selectmetdatdata]",
					"storedProcedureParameters": {
						"tablename": {
							"value": {
								"value": "@pipeline().parameters.SRCNAME",
								"type": "Expression"
							},
							"type": "String"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureSqlDatabase",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"SRCNAME": {
				"type": "string"
			}
		},
		"annotations": []
	}
}