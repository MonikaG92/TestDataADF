{
	"name": "LoadDataToAzure",
	"properties": {
		"activities": [
			{
				"name": "CopyOnpremiseSqlData",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SqlServerSource"
					},
					"sink": {
						"type": "AzureSqlSink",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "emp_id",
									"type": "String"
								},
								"sink": {
									"name": "emp_id",
									"type": "Int32"
								}
							},
							{
								"source": {
									"name": "emp_name",
									"type": "String"
								},
								"sink": {
									"name": "emp_name",
									"type": "String"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "onpremisedb",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "azuredb",
						"type": "DatasetReference"
					}
				]
			}
		],
		"parameters": {
			"Entity": {
				"type": "string"
			}
		},
		"variables": {
			"entityname": {
				"type": "String"
			}
		},
		"annotations": []
	}
}